{
    "check_interval": 600,
    "sign": true,
    "commit_based": [
        {
            "type": "misp",
            "args": [
                "--misp",
                "--misp-name misp",
                "--outputdir /opt/misp_airgap/images/"
            ]
        },
        {
            "type": "modules",
            "args": [
                "--modules",
                "--modules-name modules",
                "--outputdir /opt/misp_airgap/images/"
            ]
        },
        {
            "type": "Chinin0k/Update",
            "args": [
                "--redis",
                "--redis-name test",
                "--outputdir /opt/misp_airgap/images/"
            ]
        }
    ],
    "tag_based": [
        {
            "type": "redis",
            "args": [
                "--redis",
                "--redis-name redis",
                "--outputdir /opt/misp_airgap/images/"
            ]
        },
        {
            "type": "mysql",
            "args": [
                "--mysql",
                "--mysql-name mysql",
                "--outputdir /opt/misp_airgap/images/"
            ]
        }
    ]
}