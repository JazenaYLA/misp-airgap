{
    "check_interval": 600,
    "github": [
        {
            "name": "MISP",
            "id": "MISP/MISP",
            "mode": "commits",
            "args": [
                "--misp",
                "--misp-name",
                "MISP",
                "--outputdir",
                "/opt/misp_airgap/images/",
                "-s"
            ]
        },
        {
            "name": "MISP-modules",
            "id": "MISP/misp-modules",
            "mode": "commits",
            "args": [
                "--modules",
                "--modules-name",
                "Modules",
                "--outputdir",
                "/opt/misp_airgap/images/",
                "-s"
            ]
        }
    ],
    "apt": [
        {
            "name": "MySQL",
            "id": "mariadb-server",
            "args": [
                "--mysql",
                "--mysql-name",
                "MySQL",
                "--outputdir",
                "/opt/misp_airgap/images/",
                "-s"
            ]
        },
        {
            "name": "Redis",
            "id": "redis-server",
            "args": [
                "--redis",
                "--redis-name",
                "Redis",
                "--outputdir",
                "/opt/misp_airgap/images/",
                "-s"
            ]
        }
    ]
}